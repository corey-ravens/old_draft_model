		  ,STDEVP(position_specifics_close_on_ball_grade) AS position_specifics_close_on_ball_grade_stdev
		  ,AVG(CAST(position_specifics_hands_grade AS NUMERIC(9,7))) AS position_specifics_hands_grade_avg
		  ,STDEVP(position_specifics_hands_grade) AS position_specifics_hands_grade_stdev
		  ,AVG(CAST(position_specifics_key_and_diagnose_grade AS NUMERIC(9,7))) AS position_specifics_key_and_diagnose_grade_avg
		  ,STDEVP(position_specifics_key_and_diagnose_grade) AS position_specifics_key_and_diagnose_grade_stdev
		  ,AVG(CAST(position_specifics_man_to_man_coverage_grade AS NUMERIC(9,7))) AS position_specifics_man_to_man_coverage_grade_avg
