--Should the standardization be done by grade level as well as scout and position and trait?
--	Inherently, players the the top of the board will have higher avg position specific grades
--		So this could potentially lead to some scouts (Eric, Chad, Vince) who tend to view the higher players to have higher average grades
--			And other scouts (Andrew) who tend to view the lower players to have lower average grades



--********************  CORNERS  ********************

-- Check if temp_scout_grade_standardization_cb, if it does drop it
	IF OBJECT_ID('tempdb..##temp_scout_grade_standardization_cb') IS NOT NULL
		DROP TABLE ##temp_scout_grade_standardization_cb
-- Check if temp_grades_with_z_scores_cb, if it does drop it
	IF OBJECT_ID('tempdb..##temp_grades_with_z_scores_cb') IS NOT NULL
		DROP TABLE ##temp_grades_with_z_scores_cb

--Find each scout's average and standard deviation for position specific grades
    SELECT scout_id
          ,first_name
		  ,last_name
	      ,AVG(CAST(athletic_ability_grade AS NUMERIC(9,7))) AS athletic_ability_grade_avg 
		  ,STDEVP(athletic_ability_grade) AS athletic_ability_grade_stdev
	      ,AVG(CAST(character_grade AS NUMERIC(9,7))) AS character_grade_avg
		  ,STDEVP(character_grade) AS character_grade_stdev
	      ,AVG(CAST(competitiveness_grade AS NUMERIC(9,7))) AS competitiveness_grade_avg
		  ,STDEVP(competitiveness_grade) AS competitiveness_grade_stdev
	      ,AVG(CAST(durability_grade AS NUMERIC(9,7))) AS durability_grade_avg
		  ,STDEVP(durability_grade) AS durability_grade_stdev
	      ,AVG(CAST(explosion_grade AS NUMERIC(9,7))) AS explosion_grade_avg
		  ,STDEVP(explosion_grade) AS explosion_grade_stdev
	      ,AVG(CAST(instinct_grade AS NUMERIC(9,7))) AS instinct_grade_avg
		  ,STDEVP(instinct_grade) AS instinct_grade_stdev
	      ,AVG(CAST(playing_speed_grade AS NUMERIC(9,7))) AS playing_speed_grade_avg
		  ,STDEVP(playing_speed_grade) AS playing_speed_grade_stdev
	      ,AVG(CAST(size_grade AS NUMERIC(9,7))) AS size_grade_avg
		  ,STDEVP(size_grade) AS size_grade_stdev
	      ,AVG(CAST(speed_grade AS NUMERIC(9,7))) AS speed_grade_avg
		  ,STDEVP(speed_grade) AS speed_grade_stdev
	      ,AVG(CAST(toughness_grade AS NUMERIC(9,7))) AS toughness_grade_avg
		  ,STDEVP(toughness_grade) AS toughness_grade_stdev
	      ,AVG(CAST(position_specifics_close_on_ball_grade AS NUMERIC(9,7))) AS position_specifics_close_on_ball_grade_avg
		  ,STDEVP(position_specifics_close_on_ball_grade) AS position_specifics_close_on_ball_grade_stdev
	      ,AVG(CAST(position_specifics_hands_grade AS NUMERIC(9,7))) AS position_specifics_hands_grade_avg
		  ,STDEVP(position_specifics_hands_grade) AS position_specifics_hands_grade_stdev
	      ,AVG(CAST(position_specifics_key_and_diagnose_grade AS NUMERIC(9,7))) AS position_specifics_key_and_diagnose_grade_avg
		  ,STDEVP(position_specifics_key_and_diagnose_grade) AS position_specifics_key_and_diagnose_grade_stdev
	      ,AVG(CAST(position_specifics_man_to_man_coverage_grade AS NUMERIC(9,7))) AS position_specifics_man_to_man_coverage_grade_avg
		  ,STDEVP(position_specifics_man_to_man_coverage_grade) AS position_specifics_man_to_man_coverage_grade_stdev
	      ,AVG(CAST(position_specifics_range_grade AS NUMERIC(9,7))) AS position_specifics_range_grade_avg
		  ,STDEVP(position_specifics_range_grade) AS position_specifics_range_grade_stdev
	      ,AVG(CAST(position_specifics_run_support_tackling_grade AS NUMERIC(9,7))) AS position_specifics_run_support_tackling_grade_avg
		  ,STDEVP(position_specifics_run_support_tackling_grade) AS position_specifics_run_support_tackling_grade_stdev
	      ,AVG(CAST(position_specifics_zone_coverage_grade AS NUMERIC(9,7))) AS position_specifics_zone_coverage_grade_avg
		  ,STDEVP(position_specifics_zone_coverage_grade) AS position_specifics_zone_coverage_grade_stdev
	      ,AVG(CAST(position_specifics_special_teams_grade AS NUMERIC(9,7))) AS position_specifics_special_teams_grade_avg
		  ,STDEVP(position_specifics_special_teams_grade) AS position_specifics_special_teams_grade_stdev
	  INTO ##temp_scout_grade_standardization_cb
      FROM RavensIncoming.dbo.stratbridge_ncaa_defensive_corner_reports corners
INNER JOIN RavensIncoming.dbo.stratbridge_scouts scouts
        ON corners.scout_id=scouts.id
     WHERE report_type in (1,3,4)
  GROUP BY scout_id, first_name, last_name

--Create z-scores
    SELECT report_id
		  ,id
          ,player_id
		  ,corners.scout_id
	      ,(athletic_ability_grade-athletic_ability_grade_avg) / NULLIF(athletic_ability_grade_stdev,0) AS z_score_athletic_ability_grade 
		  ,(character_grade-character_grade_avg) / NULLIF(character_grade_stdev,0) AS z_score_character_grade
		  ,(competitiveness_grade-competitiveness_grade_avg) / NULLIF(competitiveness_grade_stdev,0) AS z_score_competitiveness_grade
		  ,(durability_grade-durability_grade_avg) / NULLIF(durability_grade_stdev,0) AS z_score_durability_grade
		  ,(explosion_grade-explosion_grade_avg) / NULLIF(explosion_grade_stdev,0) AS z_score_explosion_grade
		  ,(instinct_grade-instinct_grade_avg) / NULLIF(instinct_grade_stdev,0) AS z_score_instinct_grade
		  ,(playing_speed_grade-playing_speed_grade_avg) / NULLIF(playing_speed_grade_stdev,0) AS z_score_playing_speed_grade
		  ,(size_grade-size_grade_avg) / NULLIF(size_grade_stdev,0) AS z_score_size_grade
		  ,(speed_grade-speed_grade_avg) / NULLIF(speed_grade_stdev,0) AS z_score_speed_grade
		  ,(toughness_grade-toughness_grade_avg) / NULLIF(toughness_grade_stdev,0) AS z_score_toughness_grade
		  ,(position_specifics_close_on_ball_grade-position_specifics_close_on_ball_grade_avg) / NULLIF(position_specifics_close_on_ball_grade_stdev,0) AS z_score_position_specifics_close_on_ball_grade
		  ,(position_specifics_hands_grade-position_specifics_hands_grade_avg) / NULLIF(position_specifics_hands_grade_stdev,0) AS z_score_position_specifics_hands_grade
	      ,(position_specifics_key_and_diagnose_grade-position_specifics_key_and_diagnose_grade_avg) / NULLIF(position_specifics_key_and_diagnose_grade_stdev,0) AS z_score_position_specifics_key_and_diagnose_grade
	      ,(position_specifics_man_to_man_coverage_grade-position_specifics_man_to_man_coverage_grade_avg) / NULLIF(position_specifics_man_to_man_coverage_grade_stdev,0) AS z_score_position_specifics_man_to_man_coverage_grade
	      ,(position_specifics_range_grade-position_specifics_range_grade_avg) / NULLIF(position_specifics_range_grade_stdev,0) AS z_score_position_specifics_range_grade
	      ,(position_specifics_run_support_tackling_grade-position_specifics_run_support_tackling_grade_avg) / NULLIF(position_specifics_run_support_tackling_grade_stdev,0) AS z_score_position_specifics_run_support_tackling_grade
	      ,(position_specifics_zone_coverage_grade-position_specifics_zone_coverage_grade_avg) / NULLIF(position_specifics_zone_coverage_grade_stdev,0) AS z_score_position_specifics_zone_coverage_grade
	      ,(position_specifics_special_teams_grade-position_specifics_special_teams_grade_avg) / NULLIF(position_specifics_special_teams_grade_stdev,0) AS z_score_position_specifics_special_teams_grade
	  INTO ##temp_grades_with_z_scores_cb
      FROM RavensIncoming.dbo.stratbridge_ncaa_defensive_corner_reports corners
INNER JOIN ##temp_scout_grade_standardization_cb zscore
        ON corners.scout_id=zscore.scout_id
  ORDER BY player_id

 --Get the average standardized grades for each corner
    SELECT player_id
	      ,AVG(z_score_athletic_ability_grade) AS player_avg_z_score_athletic_ability_grade
		  ,AVG(z_score_character_grade) AS player_avg_z_score_character_grade
		  ,AVG(z_score_competitiveness_grade) AS player_avg_z_score_competitiveness_grade
		  ,AVG(z_score_durability_grade) AS player_avg_z_score_durability_grade
		  ,AVG(z_score_explosion_grade) AS player_avg_z_score_explosion_grade
		  ,AVG(z_score_instinct_grade) AS player_avg_z_score_instinct_grade
		  ,AVG(z_score_playing_speed_grade) AS player_avg_z_score_playing_speed_grade
		  ,AVG(z_score_size_grade) AS player_avg_z_score_size_grade
		  ,AVG(z_score_speed_grade) AS player_avg_z_score_speed_grade
		  ,AVG(z_score_toughness_grade) AS player_avg_z_score_toughness_grade
		  ,AVG(z_score_position_specifics_close_on_ball_grade) AS player_avg_z_score_position_specifics_close_on_ball_grade
		  ,AVG(z_score_position_specifics_hands_grade) AS player_avg_z_score_position_specifics_hands_grade
		  ,AVG(z_score_position_specifics_key_and_diagnose_grade) AS player_avg_z_score_position_specifics_key_and_diagnose_grade
		  ,AVG(z_score_position_specifics_man_to_man_coverage_grade) AS player_avg_z_score_position_specifics_man_to_man_coverage_grade
		  ,AVG(z_score_position_specifics_range_grade) AS player_avg_z_score_position_specifics_range_grade
		  ,AVG(z_score_position_specifics_run_support_tackling_grade) AS player_avg_z_score_position_specifics_run_support_tackling_grade
		  ,AVG(z_score_position_specifics_zone_coverage_grade) AS player_avg_z_score_position_specifics_zone_coverage_grade
		  ,AVG(z_score_position_specifics_special_teams_grade) AS player_avg_z_score_position_specifics_special_teams_grade
	  FROM ##temp_grades_with_z_scores_cb
  GROUP BY player_id

--********************  DEFENSIVE ENDS  ********************

-- Check if temp_scout_grade_standardization_de, if it does drop it
	IF OBJECT_ID('tempdb..##temp_scout_grade_standardization_de') IS NOT NULL
		DROP TABLE ##temp_scout_grade_standardization_de
-- Check if temp_grades_with_z_scores_de, if it does drop it
	IF OBJECT_ID('tempdb..##temp_grades_with_z_scores_de') IS NOT NULL
		DROP TABLE ##temp_grades_with_z_scores_de

--Find each scout's average and standard deviation for position specific grades
    SELECT scout_id
          ,first_name
		  ,last_name
	      ,AVG(CAST(athletic_ability_grade AS NUMERIC(9,7))) AS athletic_ability_grade_avg 
		  ,STDEVP(athletic_ability_grade) AS athletic_ability_grade_stdev
	      ,AVG(CAST(character_grade AS NUMERIC(9,7))) AS character_grade_avg
		  ,STDEVP(character_grade) AS character_grade_stdev
	      ,AVG(CAST(competitiveness_grade AS NUMERIC(9,7))) AS competitiveness_grade_avg
		  ,STDEVP(competitiveness_grade) AS competitiveness_grade_stdev
	      ,AVG(CAST(durability_grade AS NUMERIC(9,7))) AS durability_grade_avg
		  ,STDEVP(durability_grade) AS durability_grade_stdev
	      ,AVG(CAST(explosion_grade AS NUMERIC(9,7))) AS explosion_grade_avg
		  ,STDEVP(explosion_grade) AS explosion_grade_stdev
	      ,AVG(CAST(instinct_grade AS NUMERIC(9,7))) AS instinct_grade_avg
		  ,STDEVP(instinct_grade) AS instinct_grade_stdev
	      ,AVG(CAST(playing_speed_grade AS NUMERIC(9,7))) AS playing_speed_grade_avg
		  ,STDEVP(playing_speed_grade) AS playing_speed_grade_stdev
	      ,AVG(CAST(size_grade AS NUMERIC(9,7))) AS size_grade_avg
		  ,STDEVP(size_grade) AS size_grade_stdev
	      ,AVG(CAST(speed_grade AS NUMERIC(9,7))) AS speed_grade_avg
		  ,STDEVP(speed_grade) AS speed_grade_stdev
		  ,AVG(CAST(toughness_grade AS NUMERIC(9,7))) AS toughness_grade_avg
		  ,STDEVP(toughness_grade) AS toughness_grade_stdev
	      ,AVG(CAST(position_specifics_disengage_grade AS NUMERIC(9,7))) AS position_specifics_disengage_grade_avg
		  ,STDEVP(position_specifics_disengage_grade) AS position_specifics_disengage_grade_stdev
	      ,AVG(CAST(position_specifics_key_and_diagnose_grade AS NUMERIC(9,7))) AS position_specifics_key_and_diagnose_grade_avg
		  ,STDEVP(position_specifics_key_and_diagnose_grade) AS position_specifics_key_and_diagnose_grade_stdev
	      ,AVG(CAST(position_specifics_lateral_movement_grade AS NUMERIC(9,7))) AS position_specifics_lateral_movement_grade_avg
		  ,STDEVP(position_specifics_lateral_movement_grade) AS position_specifics_lateral_movement_grade_stdev
	      ,AVG(CAST(position_specifics_pass_rush_ability_grade AS NUMERIC(9,7))) AS position_specifics_pass_rush_ability_grade_avg
		  ,STDEVP(position_specifics_pass_rush_ability_grade) AS position_specifics_pass_rush_ability_grade_stdev
	      ,AVG(CAST(position_specifics_point_of_attack_grade AS NUMERIC(9,7))) AS position_specifics_point_of_attack_grade_avg
		  ,STDEVP(position_specifics_point_of_attack_grade) AS position_specifics_point_of_attack_grade_stdev
	      ,AVG(CAST(position_specifics_tackling_grade AS NUMERIC(9,7))) AS position_specifics_tackling_grade_avg
		  ,STDEVP(position_specifics_tackling_grade) AS position_specifics_tackling_grade_stdev
	      ,AVG(CAST(position_specifics_special_teams_grade AS NUMERIC(9,7))) AS position_specifics_special_teams_grade_avg
		  ,STDEVP(position_specifics_special_teams_grade) AS position_specifics_special_teams_grade_stdev
	  INTO ##temp_scout_grade_standardization_de
      FROM RavensIncoming.dbo.stratbridge_ncaa_defensive_end_reports ends
INNER JOIN RavensIncoming.dbo.stratbridge_scouts scouts
        ON ends.scout_id=scouts.id
     WHERE report_type in (1,3,4)
  GROUP BY scout_id, first_name, last_name

--Create z-scores
    SELECT report_id
		  ,id
          ,player_id
		  ,ends.scout_id
	      ,(athletic_ability_grade-athletic_ability_grade_avg) / NULLIF(athletic_ability_grade_stdev,0) AS z_score_athletic_ability_grade 
		  ,(character_grade-character_grade_avg) / NULLIF(character_grade_stdev,0) AS z_score_character_grade
		  ,(competitiveness_grade-competitiveness_grade_avg) / NULLIF(competitiveness_grade_stdev,0) AS z_score_competitiveness_grade
		  ,(durability_grade-durability_grade_avg) / NULLIF(durability_grade_stdev,0) AS z_score_durability_grade
		  ,(explosion_grade-explosion_grade_avg) / NULLIF(explosion_grade_stdev,0) AS z_score_explosion_grade
		  ,(instinct_grade-instinct_grade_avg) / NULLIF(instinct_grade_stdev,0) AS z_score_instinct_grade
		  ,(playing_speed_grade-playing_speed_grade_avg) / NULLIF(playing_speed_grade_stdev,0) AS z_score_playing_speed_grade
		  ,(size_grade-size_grade_avg) / NULLIF(size_grade_stdev,0) AS z_score_size_grade
		  ,(speed_grade-speed_grade_avg) / NULLIF(speed_grade_stdev,0) AS z_score_speed_grade
		  ,(toughness_grade-toughness_grade_avg) / NULLIF(toughness_grade_stdev,0) AS z_score_toughness_grade
		  ,(position_specifics_disengage_grade-position_specifics_disengage_grade_avg) / NULLIF(position_specifics_disengage_grade_stdev,0) AS z_score_position_specifics_disengage_grade
		  ,(position_specifics_key_and_diagnose_grade-position_specifics_key_and_diagnose_grade_avg) / NULLIF(position_specifics_key_and_diagnose_grade_stdev,0) AS z_score_position_specifics_key_and_diagnose_grade
		  ,(position_specifics_lateral_movement_grade-position_specifics_lateral_movement_grade_avg) / NULLIF(position_specifics_lateral_movement_grade_stdev,0) AS z_score_position_specifics_lateral_movement_grade
	      ,(position_specifics_pass_rush_ability_grade-position_specifics_pass_rush_ability_grade_avg) / NULLIF(position_specifics_pass_rush_ability_grade_stdev,0) AS z_score_position_specifics_pass_rush_ability_grade
	      ,(position_specifics_point_of_attack_grade-position_specifics_point_of_attack_grade_avg) / NULLIF(position_specifics_point_of_attack_grade_stdev,0) AS z_score_position_specifics_point_of_attack_grade
	      ,(position_specifics_tackling_grade-position_specifics_tackling_grade_avg) / NULLIF(position_specifics_tackling_grade_stdev,0) AS z_score_position_specifics_tackling_grade
	      ,(position_specifics_special_teams_grade-position_specifics_special_teams_grade_avg) / NULLIF(position_specifics_special_teams_grade_stdev,0) AS z_score_position_specifics_special_teams_grade
	  INTO ##temp_grades_with_z_scores_de
      FROM RavensIncoming.dbo.stratbridge_ncaa_defensive_end_reports ends
INNER JOIN ##temp_scout_grade_standardization_de zscore
        ON ends.scout_id=zscore.scout_id
  ORDER BY player_id

 --Get the average grades for each corner
    SELECT player_id
	      ,AVG(z_score_athletic_ability_grade) AS player_avg_z_score_athletic_ability_grade
		  ,AVG(z_score_character_grade) AS player_avg_z_score_character_grade
		  ,AVG(z_score_competitiveness_grade) AS player_avg_z_score_competitiveness_grade
		  ,AVG(z_score_durability_grade) AS player_avg_z_score_durability_grade
		  ,AVG(z_score_explosion_grade) AS player_avg_z_score_explosion_grade
		  ,AVG(z_score_instinct_grade) AS player_avg_z_score_instinct_grade
		  ,AVG(z_score_playing_speed_grade) AS player_avg_z_score_playing_speed_grade
		  ,AVG(z_score_size_grade) AS player_avg_z_score_size_grade
		  ,AVG(z_score_speed_grade) AS player_avg_z_score_speed_grade
		  ,AVG(z_score_toughness_grade) AS player_avg_z_score_toughness_grade
		  ,AVG(z_score_position_specifics_disengage_grade) AS player_avg_z_score_position_specifics_disengage_grade
		  ,AVG(z_score_position_specifics_key_and_diagnose_grade) AS player_avg_z_score_position_specifics_key_and_diagnose_grade
		  ,AVG(z_score_position_specifics_lateral_movement_grade) AS player_avg_z_score_position_specifics_lateral_movement_grade
		  ,AVG(z_score_position_specifics_pass_rush_ability_grade) AS player_avg_z_score_position_specifics_pass_rush_ability_grade
		  ,AVG(z_score_position_specifics_point_of_attack_grade) AS player_avg_z_score_position_specifics_point_of_attack_grade
		  ,AVG(z_score_position_specifics_tackling_grade) AS player_avg_z_score_position_specifics_tackling_grade
		  ,AVG(z_score_position_specifics_special_teams_grade) AS player_avg_z_score_position_specifics_special_teams_grade
	  FROM ##temp_grades_with_z_scores_de
  GROUP BY player_id









--Pass Rushers
    SELECT scout_id
          ,first_name
		  ,last_name
	      ,AVG(CAST(athletic_ability_grade AS NUMERIC(9,7))) AS athletic_ability_grade_avg 
		  ,STDEVP(athletic_ability_grade) AS athletic_ability_grade_stdev
	      ,AVG(CAST(character_grade AS NUMERIC(9,7))) AS character_grade_avg
		  ,STDEVP(character_grade) AS character_grade_stdev
	      ,AVG(CAST(competitiveness_grade AS NUMERIC(9,7))) AS competitiveness_grade_avg
		  ,STDEVP(competitiveness_grade) AS competitiveness_grade_stdev
	      ,AVG(CAST(durability_grade AS NUMERIC(9,7))) AS durability_grade_avg
		  ,STDEVP(durability_grade) AS durability_grade_stdev
	      ,AVG(CAST(explosion_grade AS NUMERIC(9,7))) AS explosion_grade_avg
		  ,STDEVP(explosion_grade) AS explosion_grade_stdev
	      ,AVG(CAST(instinct_grade AS NUMERIC(9,7))) AS instinct_grade_avg
		  ,STDEVP(instinct_grade) AS instinct_grade_stdev
	      ,AVG(CAST(playing_speed_grade AS NUMERIC(9,7))) AS playing_speed_grade_avg
		  ,STDEVP(playing_speed_grade) AS playing_speed_grade_stdev
	      ,AVG(CAST(size_grade AS NUMERIC(9,7))) AS size_grade_avg
		  ,STDEVP(size_grade) AS size_grade_stdev
	      ,AVG(CAST(speed_grade AS NUMERIC(9,7))) AS speed_grade_avg
		  ,STDEVP(speed_grade) AS speed_grade_stdev
		  ,AVG(CAST(toughness_grade AS NUMERIC(9,7))) AS toughness_grade_avg
		  ,STDEVP(toughness_grade) AS toughness_grade_stdev
		  ,AVG(CAST(position_specifics_disengage_grade AS NUMERIC(9,7))) AS position_specifics_disengage_grade_avg
		  ,STDEVP(position_specifics_disengage_grade) AS position_specifics_disengage_grade_stdev
		  ,AVG(CAST(position_specifics_key_and_diagnose_grade AS NUMERIC(9,7))) AS position_specifics_key_and_diagnose_grade_avg
		  ,STDEVP(position_specifics_key_and_diagnose_grade) AS position_specifics_key_and_diagnose_grade_stdev
		  ,AVG(CAST(position_specifics_lateral_movement_grade AS NUMERIC(9,7))) AS position_specifics_lateral_movement_grade_avg
		  ,STDEVP(position_specifics_lateral_movement_grade) AS position_specifics_lateral_movement_grade_stdev
		  ,AVG(CAST(position_specifics_pass_rush_ability_grade AS NUMERIC(9,7))) AS position_specifics_pass_rush_ability_grade_avg
		  ,STDEVP(position_specifics_pass_rush_ability_grade) AS position_specifics_pass_rush_ability_grade_stdev
		  ,AVG(CAST(position_specifics_point_of_attack_grade AS NUMERIC(9,7))) AS position_specifics_point_of_attack_grade_avg
		  ,STDEVP(position_specifics_point_of_attack_grade) AS position_specifics_point_of_attack_grade_stdev
		  ,AVG(CAST(position_specifics_tackling_grade AS NUMERIC(9,7))) AS position_specifics_tackling_grade_avg
		  ,STDEVP(position_specifics_tackling_grade) AS position_specifics_tackling_grade_stdev
		  ,AVG(CAST(position_specifics_special_teams_grade AS NUMERIC(9,7))) AS position_specifics_special_teams_grade_avg
		  ,STDEVP(position_specifics_special_teams_grade) AS position_specifics_special_teams_grade_stdev
	  --INTO ##temp_grades_cb
      FROM RavensIncoming.dbo.stratbridge_ncaa_defensive_pass_rusher_reports rush
INNER JOIN RavensIncoming.dbo.stratbridge_scouts scouts
        ON rush.scout_id=scouts.id
     WHERE report_type in (1,3,4)
  GROUP BY scout_id, first_name, last_name

--Safeties
    SELECT scout_id
          ,first_name
		  ,last_name
	      ,AVG(CAST(athletic_ability_grade AS NUMERIC(9,7))) AS athletic_ability_grade_avg 
		  ,STDEVP(athletic_ability_grade) AS athletic_ability_grade_stdev
	      ,AVG(CAST(character_grade AS NUMERIC(9,7))) AS character_grade_avg
		  ,STDEVP(character_grade) AS character_grade_stdev
	      ,AVG(CAST(competitiveness_grade AS NUMERIC(9,7))) AS competitiveness_grade_avg
		  ,STDEVP(competitiveness_grade) AS competitiveness_grade_stdev
	      ,AVG(CAST(durability_grade AS NUMERIC(9,7))) AS durability_grade_avg
		  ,STDEVP(durability_grade) AS durability_grade_stdev
	      ,AVG(CAST(explosion_grade AS NUMERIC(9,7))) AS explosion_grade_avg
		  ,STDEVP(explosion_grade) AS explosion_grade_stdev
	      ,AVG(CAST(instinct_grade AS NUMERIC(9,7))) AS instinct_grade_avg
		  ,STDEVP(instinct_grade) AS instinct_grade_stdev
	      ,AVG(CAST(playing_speed_grade AS NUMERIC(9,7))) AS playing_speed_grade_avg
		  ,STDEVP(playing_speed_grade) AS playing_speed_grade_stdev
	      ,AVG(CAST(size_grade AS NUMERIC(9,7))) AS size_grade_avg
		  ,STDEVP(size_grade) AS size_grade_stdev
	      ,AVG(CAST(speed_grade AS NUMERIC(9,7))) AS speed_grade_avg
		  ,STDEVP(speed_grade) AS speed_grade_stdev
		  ,AVG(CAST(toughness_grade AS NUMERIC(9,7))) AS toughness_grade_avg
		  ,STDEVP(toughness_grade) AS toughness_grade_stdev
