		  ,AVG(CAST(position_specifics_play_making_grade AS NUMERIC(9,7))) AS position_specifics_play_making_grade_avg
		  ,STDEVP(position_specifics_play_making_grade) AS position_specifics_play_making_grade_stdev
		  ,AVG(CAST(position_specifics_release_grade AS NUMERIC(9,7))) AS position_specifics_release_grade_avg
		  ,STDEVP(position_specifics_release_grade) AS position_specifics_release_grade_stdev
		  ,AVG(CAST(position_specifics_routes_rating AS NUMERIC(9,7))) AS position_specifics_routes_grade_avg
		  ,STDEVP(position_specifics_routes_rating) AS position_specifics_routes_grade_stdev
		  ,AVG(CAST(position_specifics_run_after_catch_grade AS NUMERIC(9,7))) AS position_specifics_run_after_catch_grade_avg
		  ,STDEVP(position_specifics_run_after_catch_grade) AS position_specifics_run_after_catch_grade_stdev
		  ,AVG(CAST(position_specifics_special_teams_grade AS NUMERIC(9,7))) AS position_specifics_special_teams_grade_avg
		  ,STDEVP(position_specifics_special_teams_grade) AS position_specifics_special_teams_grade_stdev
	--INTO ##temp_grades_cb
      FROM RavensIncoming.dbo.stratbridge_ncaa_wideout_reports wide
INNER JOIN RavensIncoming.dbo.stratbridge_scouts scouts
        ON wide.scout_id=scouts.id
     WHERE report_type in (1,3,4)
  GROUP BY scout_id, first_name, last_name
