Force is mass times acceleration  
-------------------------------*/

-- Check if ##temp_works_ten exists, if it does drop it
IF OBJECT_ID('tempdb..##temp_works_ten') IS NOT NULL
	DROP TABLE ##temp_works_ten

    SELECT workout_id
	      ,workout_date
		  ,workout_description
		  ,workout_type
		  ,workout_type_for_models
	      ,player_id
		  ,nfl_player_id
		  ,college_player_id
		  ,NULL AS measurable_type_id
		  ,'Work10' AS code
		  ,reliability_id
		  ,reliability_description
		  ,translation
		  ,attempt
	      ,([weight]*0.453592)*POWER((2*(10*0.9144)/ten_time),2)*10 AS value
	  INTO ##temp_works_ten
      FROM ##temp_advanced_pivot
	 WHERE ten_time IS NOT NULL
	   AND [weight] IS NOT NULL
  ORDER BY workout_id

