		  ,workout_type_for_models
	      ,player_id
		  ,nfl_player_id
		  ,college_player_id
		  ,NULL AS measurable_type_id
		  ,'Velocity40' AS code
		  ,reliability_id
		  ,reliability_description
		  ,translation
		  ,attempt
	      ,2*(20-(2*(10-((2*(10*0.9144)/ten_time)*(twenty_time-ten_time)))/(twenty_time-ten_time)))/(forty_time-twenty_time) AS value
	  INTO ##temp_velocities_forty
      FROM ##temp_advanced_pivot
	 WHERE ten_time IS NOT NULL
	   AND twenty_time IS NOT NULL
	   AND forty_time IS NOT NULL
  ORDER BY workout_id

--SELECT * FROM ##temp_velocities_forty

