		  ,workout_type_for_models
	      ,player_id
		  ,nfl_player_id
		  ,college_player_id
		  ,NULL AS measurable_type_id
		  ,'Velocity20' AS code
		  ,reliability_id
		  ,reliability_description
		  ,translation
		  ,attempt
	      ,2*(10-((2*(10*0.9144)/ten_time)*(twenty_time-ten_time)))/(twenty_time-ten_time)/ten_time AS value
	  INTO ##temp_velocities_twenty
      FROM ##temp_advanced_pivot
	 WHERE ten_time IS NOT NULL
	   AND twenty_time IS NOT NULL
  ORDER BY workout_id

